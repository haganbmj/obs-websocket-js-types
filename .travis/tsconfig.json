{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "importHelpers": true,
    "lib": [
      "es2017"
    ],
    "listFiles": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2017",
    "traceResolution": false,
    "types": [
      "node"
    ]
  },
  "include": [
    "**/*.ts"
  ],
  "compileOnSave": false
}
